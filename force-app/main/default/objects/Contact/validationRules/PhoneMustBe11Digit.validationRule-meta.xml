<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PhoneMustBe11Digit</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Phone, &quot;(\\d{11})?&quot;))</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>PhoneMustBe11Digit</errorMessage>
</ValidationRule>
